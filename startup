_G.version = "0.1"
_G.syscolor = colors.green
_G.highlightcolor = colors.lime

term.clear()
term.setCursorPos(1,1)
print("BOOTING")
print("LOADING APIS")
apiDir = "rSystem/rAPIs/"
apis = fs.list(apiDir)
for i,api in pairs(apis) do
  os.loadAPI(apiDir..api)
end
print("LOADING COMPLETE")
shell.run("rSystem/login")

