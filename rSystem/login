term.setBackgroundColor(_G.syscolor)
term.clear()
rtext.center("RavenOS v".._G.version, 2)
w,h = term.getSize()
paintutils.drawLine(w/2-10, 5, w/2+10, 5, _G.highlightcolor)
paintutils.drawLine(w/2-10, 7, w/2+10, 7, _G.highlightcolor)
term.setCursorPos(w/2-10-string.len("Username: "), 5)
print("Username:")
term.setCursorPos(w/2-10-string.len("Password "), 7)
sleep(3)

